{
  "name": "ggfonts",
  "type": "module",
  "version": "1.0.1",
  "description": "A CLI tool to search and install Google Fonts",
  "main": "index.js",
  "bin": {
    "ggfonts": "index.js"
  },
  "scripts": {
    "test-update": "npx . update",
    "test-config": "npx . config -k AN_INVALID_API_KEY",
    "test-config-default": "npx . config",
    "test-install": "npx . install rains",
    "test-search": "npx . search fira",
    "test": "npx .",
    "postinstall": "ggfonts -h"
  },
  "keywords": [
    "font",
    "google",
    "fonts",
    "google-fonts",
    "command-line",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/antruongnguyen/ggfonts.git"
  },
  "author": "An Nguyen <annguyen.apps@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "commander": "^9.4.0",
    "fuzzy-search": "^3.2.1"
  },
  "bugs": {
    "url": "https://github.com/antruongnguyen/ggfonts/issues"
  },
  "homepage": "https://github.com/antruongnguyen/ggfonts#readme"
}
